package com.csye6225.spring2020.courseservice.Model;

import java.util.List;

public class Course {
	
	private String courseName;
	private List<String> Lectures;
	private String board;
	private String roster;
	private List<String> enrolledStudents;
	private String professorName;
	private String studentTA;
	private int credit;

	public Course() {

	}

	public Course(String courseName, List<String> lectures, String board, String roster, List<String> enrolledStudents,
			String professorName, String studentTA, int credit) {
		this.courseName = courseName;
		Lectures = lectures;
		this.board = board;
		this.roster = roster;
		this.enrolledStudents = enrolledStudents;
		this.professorName = professorName;
		this.studentTA = studentTA;
		this.credit = credit;
	}
	
	

}


